<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hrd PUBLIC "-//Cail Lomecb//DTD Colorer HRD take5//EN"
  "http://colorer.sf.net/2003/hrd.dtd">
<hrd xmlns="http://colorer.sf.net/2003/hrd">

  <assign name="def:Text" fore="#7" back="#8"/>
  <assign name="def:HorzCross"  fore="#F" back="#0"/>
  <assign name="def:VertCross" fore="#F" back="#0"/>
  <assign name="def:Syntax"/>

  <assign name="def:Number"    fore="#D"/>
  <assign name="def:NumberSuffix" fore="#3"/>

  <assign name="def:String" fore="#E"/>
  <assign name="def:StringContent" fore="#C"/>
  <assign name="def:StringEdge" fore="#E"/>
  <assign name="def:Character" fore="#E"/>
  <assign name="def:CharacterContent" fore="#C"/>

  <assign name="def:Comment" fore="#6"/>
  <assign name="def:CommentContent" fore="#B"/>
  <assign name="def:CommentEdge"    fore="#3"/>
  <assign name="def:CommentDoc"     fore="#3"/>
  <assign name="def:CommentDocContent" fore="#B"/>
  <assign name="def:CommentDocEdge" fore="#3"/>

  <assign name="def:Symbol" fore="#F"/>
  <assign name="def:SymbolStrong" fore="#F"/>
  <assign name="def:Prefix" fore="#F"/>
  <assign name="def:PrefixStrong" fore="#E"/>

  <assign name="def:Operator" fore="#C"/>

  <assign name="def:Keyword"       fore="#B"/>
  <assign name="def:KeywordStrong" fore="#C"/>
  <assign name="def:FunctionKeyword" fore="#B"/>
  <assign name="def:DeprecatedKeyword" fore="#7"/>
  <assign name="def:InterfaceKeyword" fore="#7"/>
  <assign name="def:ClassKeyword" fore="#B"/>
  <assign name="def:StructKeyword" fore="#F"/>
  <assign name="def:TypeKeyword" fore="#B"/>

  <assign name="def:Function" fore="#B" />
  <assign name="def:Register" fore="#C"/>
  <assign name="def:Constant" fore="#D"/>
  <assign name="def:Var" fore="#F"/>
  <assign name="def:VarStrong" fore="#E"/>
  <assign name="def:Identifier" fore="#A"/>
  <assign name="def:BooleanConstant" fore="#D"/>

  <assign name="def:Directive" fore="#C"/>
  <assign name="def:DirectiveContent" fore="#F"/>
  <assign name="def:DirectiveEdge" fore="#C"/>
  <assign name="def:Parameter" fore="#A"/>
  <assign name="def:ParameterStrong" fore="#5"/>
  <assign name="def:ParameterUnknown" fore="#B"/>

  <assign name="def:Tag" fore="#C"/>
  <assign name="def:OpenTag" fore="#F"/>
  <assign name="def:CloseTag" fore="#F"/>

  <assign name="def:Label" fore="#D"/>
  <assign name="def:LabelStrong" fore="#1" back="#F"/>

  <assign name="def:Insertion" fore="#F" back="#0"/>

  <assign name="def:Error" fore="#0" back="#4"/>
  <assign name="def:ErrorText" fore="#D"/>

  <assign name="def:TODO" fore="#C"/>
  <assign name="def:Debug" fore="#C" back="#6"/>

  <assign name="def:URI" fore="#A"/>
  <assign name="def:Path" fore="#C"/>
  <assign name="def:URL" fore="#B"/>
  <assign name="def:EMail" fore="#E"/>

  <assign name="def:Date" fore="#E"/>
  <assign name="def:Time" fore="#A"/>

  <assign name="def:PairStart" fore="#4"/>
  <assign name="def:PairEnd" fore="#4" />
  <assign name="def:PairStrongStart" fore="#4"/>
  <assign name="def:PairStrongEnd" fore="#4"/>

  <assign name="def:Whitespace" fore="#6"/>

  <!-- language-specific -->

  <assign name="markdown:Text" fore="#F"/>
  <assign name="markdown:Code" fore="#E"/>
  <assign name="markdown:ListMark" fore="#D"/>
  <assign name="markdown:ListText" fore="#F"/>
  <assign name="markdown:HeadMark" fore="#5"/>
  <assign name="markdown:HeadText" fore="#F"/>
  <assign name="markdown:FenceMark" fore="#F"/>
  <assign name="markdown:StrongText" fore="#F"/>

  <assign name="shell:command.sys" fore="#B"/>
  <assign name="shell:symb" fore="#C"/>

  <assign name="css:DeprecatedPropertyName" fore="#6"/>
  <assign name="css:TagName" fore="#A"/>
  <assign name="css:PropertyName" fore="#B"/>
  <assign name="css:PropertyValue" fore="#B"/>
  <assign name="css:UnitSuffix" fore="#B"/>
  <assign name="css:Symbol" fore="#A"/>

  <assign name="html:htmlOpenTag" fore="#C"/>

  <assign name="sql:tsqlWord" fore="#C"/>
</hrd>
